{
  "expo": {
    "name": "F-ZERO クリア管理",
    "slug": "fzero-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1E3A8A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "fzerotracker",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fzerocomplete.tracker",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "このアプリは、より関連性の高い広告を表示するためにデバイス識別子を使用します。",
        "NSCameraUsageDescription": "スクリーンショットを撮影するためにカメラへのアクセスが必要です。",
        "NSPhotoLibraryUsageDescription": "ゲームの記録にスクリーンショットを保存するために写真ライブラリへのアクセスが必要です。"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1E3A8A"
      },
      "package": "com.fzerocomplete.tracker",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_MEDIA_IMAGES"
      ]
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "kotlinVersion": "1.7.20",
            "ndkVersion": "25.2.9519653"
          }
        }
      ],
      "expo-router",
      "expo-sqlite"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f0a12635-0b6a-4c7f-87c9-c1ccf4946716"
      }
    },
    "owner": "kurotanx07"
  }
}
